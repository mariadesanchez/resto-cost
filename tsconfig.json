{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "src/actions/order/delete-order-by-id.ts", "src/actions/search/SearchProducts.ts", "src/components/ui/top-menu/TopMenu.tsx", "src/app/(shop)/admin/ingrediente/[slug]", "src/app/(shop)/admin/precios/[slug]", "src/app/(shop)/admin/precios/[slug].tsx", "src/app/(shop)/admin/precios/[slug]/listPrice", "src/app/(shop)/admin/precios/[slug]/listPrice"],
  "exclude": ["node_modules"]
}
